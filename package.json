{
  "name": "client-auth",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:security": "eslint . --ext .js --config .eslintrc.js --format table",
    "security:check": "npm audit && npm run lint:security",
    "security:fix": "npm audit fix && npm run lint:fix",
    "prestart": "npm run lint:security",
    "predev": "npm run lint"
  },
  "keywords": ["authentication", "nodejs", "express", "security", "jwt", "bcrypt"],
  "author": "Carlos Madrid",
  "license": "ISC",
  "description": "Client authentication module with comprehensive security linting",
  "dependencies": {
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-node": "^11.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AdamsDaniel/client-auth-main.git"
  },
  "bugs": {
    "url": "https://github.com/AdamsDaniel/client-auth-main/issues"
  },
  "homepage": "https://github.com/AdamsDaniel/client-auth-main#readme"
}
